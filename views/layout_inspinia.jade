doctype html
html
    head
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1.0')
        title INSPINIA | #{title}
        link(href='/assets/components/bootstrap/css/bootstrap.min.css?3.3.6', rel='stylesheet')
        link(href='/assets/components/font-awesome/css/font-awesome.min.css?4.6.3', rel='stylesheet')
